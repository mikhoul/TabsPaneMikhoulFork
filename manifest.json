{
  "name": "Visual Tab Manager",
  "version": "0.8",
  "manifest_version": 2,
  "description": "Manage and search all your tabs from one tab",
  "homepage_url": "https://github.com/mikhoul/TabsPaneMikhoulFork",
  "minimum_chrome_version":"30.0.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["js/background.js", "js/jquery/jquery-3.3.1.min.js"],
    "persistent": true
  },
  "omnibox": {
    "keyword": "ls"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Visual Tab Manager"
  },
  "permissions": [
    "tabs",
    "history",
    "<all_urls>"
  ],
  
  "web_accessible_resources": [
       "img/*"
     ],
  
  "offline_enabled": true
}